{
  "name": "impossiblefish",
  "version": "0.0.1",
  "description": "Model development",
  "main": "index.js",
  "config": {
    "client_root_dir": "SET IN .npmrc FILE",
    "server_root_dir": "SET IN .npmrc FILE"
  },
  "scripts": {
    "dev": "webpack-dev-server --devtool source-map --inline --progress --colors --content-base public",
    "debug": "webpack --devtool source-map --progress --colors",
    "test": "karma start --single-run --no-auto-watch karma.config.js",
    "test-watch": "karma start --auto-watch --no-single-run karma.config.js",
    "prebuild": "rm -f $npm_package_config_client_root_dir/dist/*",
    "build": "NODE_ENV=production webpack -p",
    "predeploy": "rm $npm_package_config_server_root/public/* & cp $npm_package_config_client_root_dir/dist/* $npm_package_config_server_root_dir/public/",
    "deploy": ". $npm_package_config_server_root_dir/scripts/deploy_to_server.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/numical/impossiblefish.git"
  },
  "keywords": [
    "javascript",
    "tooling"
  ],
  "author": "Mike Evans",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/numical/impossiblefish/issues"
  },
  "homepage": "https://github.com/numical/impossiblefish#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^0.3.3",
    "css-loader": "^0.23.0",
    "file-loader": "^0.8.5",
    "karma": "^0.13.21",
    "karma-mocha": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.24",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "phantomjs-polyfill": "0.0.1",
    "phantomjs-prebuilt": "^2.1.3",
    "postcss-loader": "^0.8.0",
    "react-addons-test-utils": "^0.14.7",
    "script-loader": "^0.6.1",
    "style-loader": "^0.13.0",
    "uglify-js": "^2.6.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "es6-object-assign": "^1.0.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.2.1",
    "redux": "^3.2.1",
    "seedrandom": "^2.4.2",
    "string.prototype.startswith": "^0.2.0"
  }
}
