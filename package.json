{
  "name": "impossiblefish",
  "version": "0.0.1",
  "description": "Model development",
  "main": "index.js",
  "config": {
    "client_root_dir": "/home/mikee/sandbox/impossiblefish",
    "server_root_dir": "/home/mikee/sandbox/impossiblefishserver.git"
  },
  "scripts": {
    "build": "webpack --devtool source-map --progress --colors",
    "debug": "webpack-dev-server --devtool source-map --inline --progress --colors --content-base public",
    "predeploy": "NODE_ENV=production webpack -p",
    "deploy:clean": "rm $npm_package_config_server_root_dir/public/* & echo 'old deployment deleted'",
    "deploy:copy": "cp $npm_package_config_client_root_dir/dist/* $npm_package_config_server_root_dir/public/ & echo 'new deployment copied'",
    "deploy:commit": ".$npm_package_config_server_root_dir/git_add.sh & .$npm_package_config_server_root_dir/git_commit.sh & echo 'new deployment committed'",
    "deploy:push": ".$npm_package_config_server_root_dir/git_push.sh & 'new deployment pushed to server'",
    "deploy" : "npm run deploy:clean & npm run deploy:copy & npm run deploy:commit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/numical/impossiblefish.git"
  },
  "keywords": [
    "javascript",
    "tooling"
  ],
  "author": "Mike Evans",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/numical/impossiblefish/issues"
  },
  "homepage": "https://github.com/numical/impossiblefish#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.1",
    "babel-core": "^6.3.15",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "copy-webpack-plugin": "^0.3.3",
    "css-loader": "^0.23.0",
    "file-loader": "^0.8.5",
    "postcss-loader": "^0.8.0",
    "style-loader": "^0.13.0",
    "uglify-js": "^2.6.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.2.1",
    "redux": "^3.2.1"
  }
}
